    }
  },
  "autoload-dev": {
    "psr-4": {
      "Verinder\\PrestashopWebService\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Verinder\\PrestashopWebService\\PrestashopWebServiceProvider"
